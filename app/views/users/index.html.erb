<h2>All Users</h2>

<div>
    <ol>
    <% @users.each do |user| %>
    <li>
    <%= user.name %> | 
    <% if %>
    <%= link_to "Send Friend Request", user_send_friend_request_path(:id => user.id, :user_id => current_user.id), method: :post %>
    <%end%>    
    </li>
    <% end %>
    </ol>
</div>
