<h2>All Users</h2>

<div>
    <ol>
    <% @users.each do |user| %>
    <% if current_user != user%>    
    <li>
    <%= user.name %> | 
    <%= link_to "Send Friend Request", send_friend_request_path(:friend_id => user.id, :user_id => current_user.id), method: :post %>
    </li>
    <%end%>    
    <% end %>
    </ol>
</div>
